{
  "name": "bernardosecades/php-skeleton-kata",
  "description": "php skeleton kata",
  "keywords": ["php7", "kata", "TDD", "skeleton"],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Bernardo Secades",
      "email": "bernardosecades@gmail.com"
    }
